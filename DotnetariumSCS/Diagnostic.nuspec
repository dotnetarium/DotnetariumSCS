<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata minClientVersion="2.8">
    <id>DotnetariumSCS</id>
    <version>1.0.0</version>
    <authors>Dmitry Balikhin</authors>
    <license type="expression">LGPL-3.0-or-later</license>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <licenseUrl>https://licenses.nuget.org/LGPL-3.0-or-later</licenseUrl>
    <iconUrl>https://raw.githubusercontent.com/dotnetarium/DotnetariumSCS/main/icon.png</iconUrl>
    <projectUrl>https://github.com/dotnetarium/DotnetariumSCS</projectUrl>
    <description>Security static code analyzer for .NET based on Security Code Scan (Jaroslav Lobačevski)</description>
    <releaseNotes>https://github.com/dotnetarium/DotnetariumSCS/releases</releaseNotes>
    <copyright>© DotnetariumSCS</copyright>
    <tags>DotnetariumSCS Dotnetarium SCS SecurityCodeScan Security.Code.Scan Security Code Scan StaticAnalysis Vulnerability Analyzer .NET Roslyn OWASP Injection XSS CSRF XXE SQLi</tags>
    <developmentDependency>true</developmentDependency>
    <repository type="git" url="https://github.com/dotnetarium/DotnetariumSCS" commit="$repositoryCommit$" />
    <readme>README.md</readme>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\dotnet and language specific analyzers in either analyzers\dotnet\cs or analyzers\dotnet\vb -->
  <files>
    <file src="DotnetariumSCS.dll" target="analyzers\dotnet" />
    <file src="YamlDotNet.dll" target="analyzers\dotnet" />
    <file src="README.md" target="\" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>
